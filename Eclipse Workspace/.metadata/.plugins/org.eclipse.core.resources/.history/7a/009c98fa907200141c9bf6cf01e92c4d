
public class Misc {

	public static void main(String[] args) {
		
	    double p = 10.1; 
	    double q = 5.5; 
	     
	    if ( p > 10.0 )  
	    { 
	        q += 1.0; 
	        p = 3.0; 
	    } 
	    else if ( p > 5.0 )  
	    { 
	        q += 2.0; 
	    } 
	    else if ( p > 2.0 )  
	    { 
	        q += 3.0; 
	        p = 9.0; 
	    } 
	    else  
	    { 
	        q += 4.0;  
	        p = 0.0; 
	    }
	    
	    System.out.println(q);
	}
	
}
